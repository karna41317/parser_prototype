{
  "env": {
    "amd": false,
    "node": true
  },

  "rules": {
    //0 = no warning, 1 = warning, 2 = error
    "block-scoped-var": 2,
    "brace-style": [2, "1tbs", { "allowSingleLine": false }],
    "camelcase": 1,
    "consistent-return": 0,
    "curly": 0,
    "default-case": 0,
    "dot-notation": 2,
    "eqeqeq": 2,
    "guard-for-in": 2,
    "key-spacing": 0,
    "max-nested-callbacks": [2, 2],
    "new-cap": 2,
    "no-alert": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-catch-shadow": 0,
    "no-comma-dangle": 0,
    "no-cond-assign": 2,
    "no-constant-condition": 2,
    "no-debugger": 2,
    "no-dupe-keys": 2,
    "no-else-return": 2,
    "no-empty": 0,
    "no-empty-class": 0,
    "no-eq-null": 0,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 0,
    "no-extra-semi": 2,
    "no-extra-strict": 2,
    "no-fallthrough": 0,
    "no-floating-decimal": 2,
    "no-func-assign": 2,
    "no-implied-eval": 0,
    "no-inner-declarations": [2, "both"],
    "no-invalid-regexp": 2,
    "no-iterator": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 0,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-negated-in-lhs": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-obj-calls": 2,
    "no-plusplus": 0,
    "no-proto": 2,
    "no-regex-spaces": 0,
    "no-reserved-keys": 0,
    "no-script-url": 2,
    "no-shadow": 2,
    "no-sparse-arrays": 0,
    "no-trailing-spaces": 1,
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "no-unreachable": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": 1,
    "no-use-before-define": [ 2, "nofunc" ],
    "quotes": 0,
    "semi": [ 2, "always" ],
    "space-after-keywords": 2,
    "strict": 2,
    "use-isnan": 0,
    "valid-jsdoc": 0,
    "valid-typeof": 2,
    "valid-typeof": 2,
    "vars-on-top": 0,
    "wrap-iife": 0
  }
}
